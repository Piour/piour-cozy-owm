{
  "name": "piour-cozy-owm",
  "version": "0.5.1",
  "engines": [
    "node >= 0.8.0"
  ],
  "description": "Weather of your favorites cities !",
  "repository": {
    "type": "git",
    "url": "https://github.com/Piour/piour-cozy-owm.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.3.3",
    "americano-cozy": "0.2.2",
    "async": "0.2.9"
  },
  "devDependencies": {
    "should": "= 1.2.0",
    "mocha": "= 1.6.0",
    "chai": "= 1.3.0"
  },
  "scripts": {
    "test": "cake tests",
    "start": "coffee server.coffee"
  },
  "cozy-displayName": "Weather",
  "cozy-permissions": {
    "City": {
      "description": "Allow you to manage your weather forecast"
    }
  },
  "cozy-widget": "/apps/piour-owm/widget.html"
}
